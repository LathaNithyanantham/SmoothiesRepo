<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.latha.smoothiesapplication">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/main_img"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity">
        </activity>
        <activity android:name=".SignUp">
        </activity>
        <activity android:name=".MainPage"
            android:theme="@style/AppTheme.NoActionBar">
            </activity>
        <activity android:name=".DirectedTo">
        </activity>
        <activity android:name=".SelectItems">
        </activity>
        <activity android:name=".FinalPage"></activity>
        <activity android:name=".Progress"></activity>
        <activity android:name=".Register"></activity>
    </application>


</manifest>